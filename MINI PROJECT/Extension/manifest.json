{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "/icons/cross.png",
      "32": "/icons/cross.png",
      "48": "/icons/cross.png",
      "128": "/icons/cross.png"
  },
  "web_accessible_resources": [
    "images/*.png",
    "styles/*.css",
    "scripts/*.js"
]
  

}
